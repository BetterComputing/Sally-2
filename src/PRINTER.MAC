;
;
;	... PARALLEL PRINTER OUTPUT ROUTINE ...
;
CENTOUT:

	LD	A,C
	OUT	(PRINTER),A	;OUTPUT DATA BYTE TO PRINTER
	EX	(SP),HL
	EX	(SP),HL
	LD	A,25
	OUT	(STROBE),A
CENT2:	DEC	A
	JR	NZ,CENT2
	OUT	(STROBE),A	;TWANG THE STROBE
	RET
;
;
;
CENTRDY:
	LD	HL,(PMASKS)
	IN	A,(PRINTER)
	AND	L		;MASK BITS WITH 'AND'
	XOR	H		;COMPARE WITH 'XOR'
	RET			;ACC=0 IF PRINTER IS READY
;
;
;
;
